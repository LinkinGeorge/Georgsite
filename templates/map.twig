{% extends "base.twig" %}

{% block body %}
    <body class="{{body_class}} static map-body" data-template="base.twig">
{% endblock %}

{% block nav %}
    <nav class="navbar navbar-fixed-top map-nav" role="navigation">
{% endblock %}

{% block content %}
    <div class="map">
        <object class="fullscreen-map" type="image/svg+xml" data="{{site.theme.link}}/images/europe3.svg">
            Your browser does not support SVG
        </object>
    </div>
    <script>
        $(document).ready(function(){
            $('[data-toggle="tooltip"]').tooltip(); 
        });
    </script>
{% endblock content %}

{% block footer %}
    
{% endblock footer %}